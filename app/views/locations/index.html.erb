<div class="container">
  <input class="search" placeholder="My Location">
  <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1474.0882405079883!2d-71.05479665!3d42.36007805!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89e37085d9d7b96f%3A0x56f92c46480c9be0!2sFaneuil+Hall+Marketplace!5e0!3m2!1sen!2sus!4v1399682281226" width="100%" height="200" frameborder="0" style="border:0"></iframe>
  <% @artists.each do |t| %>
    <div class="artist_prof">
      <% if t[:photo_url] %>
        <img src="<%= t.photo_url  %>">
      <% else %>
        <%= image_tag 'user_profile.jpg' %>
      <% end %>
        <ul>
          <li><h3><%= t.artist %></h3></li>
          <li><h4><%= t.genre %></h4></li>
          <li><h4>Rating: <%= t.rating %></h4></li>
        </ul>
        <div style="clear: both"></div>
    </div>
  <% end %>
</div>
